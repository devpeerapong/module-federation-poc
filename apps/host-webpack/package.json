{
  "name": "host-webpack",
  "version": "1.0.0",
  "description": "Fastify app with EJS template",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsx watch src/server/index.ts\" \"pnpm:build:dev --watch\"",
    "start": "tsx src/server/index.ts",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "serve": "pnpm start"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.2.0",
    "@fastify/view": "^11.1.0",
    "@module-federation/enhanced": "^0.17.1",
    "@types/ejs": "^3.1.5",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "~19.1.7",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "css-loader": "^7.1.2",
    "ejs": "^3.1.10",
    "fastify": "^5.4.0",
    "mini-css-extract-plugin": "^2.9.3",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "shared": "workspace:*",
    "tsx": "^4.7.0",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "keywords": [
    "fastify",
    "ejs"
  ],
  "author": "",
  "license": "ISC"
}
